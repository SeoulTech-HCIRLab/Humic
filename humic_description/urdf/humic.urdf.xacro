            </geometry>
            <material name="white"/>
        </visual>

        <collision>
            <origin xyz="-0.1458 0.3375 -0.4482" rpy="0 0 0"/>
            <geometry>
                <mesh
                    filename="package://humic_description/meshes/STL/r_foreArm.stl" scale="0.027 0.027 0.027"/>
