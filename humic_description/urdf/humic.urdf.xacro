        </inertial>
    </link>

    <joint name="r_foreArm_joint" type="revolute">
        <axis xyz="0 0 -1" />
        <limit velocity="6.5" effort="1000" lower="${-pi}" upper="${pi}" />
        <dynamics damping="0.7"/>
        <origin xyz="0 -0.0297 -0.243" rpy="0 0 0"/>
